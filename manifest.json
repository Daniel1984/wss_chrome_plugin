{
  "name": "WSS",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Web Scraping Service",
  "homepage_url": "https://metabnb.com",
  "options_page": "options.html",
  "background": {
    "page": "background.html",
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon-34.png",
    "default_title": "Web Scraping Service"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab",
    "background"
  ],
  "icons": {
    "128": "icon-128.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
