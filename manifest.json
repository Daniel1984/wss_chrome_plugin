{
  "name": "WSS",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Web Scraping Service",
  "homepage_url": "http://www.marriott.com",
  "web_accessible_resources": [
    "*.png",
    "banner.png",
    "./banner.png"
  ],
  "background": {
    "scripts": [
      "background.js"
    ],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html",
    "default_title": "Web Scraping Service"
  },
  "permissions": [
    "https://*/*",
    "http://*/*",
    "activeTab",
    "background"
  ]
}
